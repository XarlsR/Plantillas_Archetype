<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  
  <packaging>maven-archetype</packaging>
  <groupId>dev.xarlsr.archetype</groupId>
  <artifactId>xarlsr-hibernate-archetype</artifactId>
  <version>1.0.1
    
  </version>
  <name>xarlsr-hibernate-archetype</name>
  <url>https://github.com/XarlsR/xarlsr-utilidades.git</url>

  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <project.reporting.sourceEncoding>UTF-8</project.reporting.sourceEncoding>
    <java.version>17</java.version>
    <maven.compiler.source>17</maven.compiler.source>
    <maven.compiler.target>17</maven.compiler.target>
    </properties>
  
    <build>
        <extensions>
            <extension>
                <!-- https://mvnrepository.com/artifact/org.apache.maven.archetype/archetype-packaging -->
                <groupId>org.apache.maven.archetype</groupId>
                <artifactId>archetype-packaging</artifactId>
                <version>3.2.1</version>
            </extension>
        </extensions>
        <!--....-->
    </build>

</project>
